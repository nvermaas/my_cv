t AstroBaseCard from \"./AstroBaseCard\";\r\nimport EnergyViewCard from \"./EnergyViewCard\";\r\nimport EnergyServerCard from \"./EnergyServerCard\";\r\n\r\nconst base_url =\r\n    process.env.NODE_ENV === \"development\"\r\n        ? \"http://localhost:3000\"\r\n        : \"https://uilennest.net/cv\"\r\n\r\nconst projects = [\r\n    {\"name\": \"astroview\", \"type\": \"frontend_reactjs\", \"card\": \"AstroViewCard\", \"thumbnail\": \"/assets/astroview.png\"},\r\n    {\"name\": \"astrobase\", \"type\": \"backend_django\", \"card\": \"AstroBaseCard\", \"thumbnail\": \"/assets/astrobase.png\"},\r\n    {\"name\": \"energy_server\", \"type\": \"backend_django\", \"card\": \"EnergyServerCard\", \"thumbnail\": \"/assets/my_energy_server.png\"},\r\n    {\"name\": \"energy_view\", \"type\": \"frontend_reactjs\", \"card\": \"EnergyViewCard\", \"thumbnail\": \"/assets/my_energy_view.png\"}\r\n]\r\n\r\nconst PROJECT_CARDS = {\r\n    astroview: AstroViewCard,\r\n    astrobase: AstroBaseCard,\r\n    energy_view: EnergyViewCard,\r\n    energy_server: EnergyServerCard\r\n};\r\n\r\nexport default function Projects(props) {\r\n    const [ my_state ] = useGlobalReducer()\r\n    return (\r\n        <div className=\"App\">\r\n            <header>\r\n                <Container fluid>\r\n                    <Row>\r\n                        {\r\n                            projects.map((project) => {\r\n                                if ((props.type === project.type) || (props.type === 'all')) {\r\n                                    const DynamicProjectCard = PROJECT_CARDS[project.name]\r\n                                    let image_url = base_url + project.thumbnail\r\n                                    return (\r\n                                        <Col lg={true}>\r\n                                            <DynamicProjectCard image={image_url}/>\r\n                                        </Col>\r\n                                    );\r\n                                }\r\n                            })\r\n                        }\r\n                    </Row>\r\n                </Container>\r\n            </header>\r\n        </div>\r\n    );\r\n}","R:\\source\\javascript\\my_cv\\src\\routes\\portfolio\\ProjectTiles.js",["138","139"],"import React from 'react';\r\nimport { Container, Row, Col } from 'react-bootstrap';\r\nimport ProjectThumbnail from './ProjectThumbnail';\r\nimport AstroViewCard from \"./AstroViewCard\";\r\nimport AstroBaseCard from \"./AstroBaseCard\";\r\nimport EnergyCard from \"./EnergyCard\";\r\n\r\nconst PROJECT_CARDS = {\r\n    astroview: AstroViewCard,\r\n    astrobase: AstroBaseCard,\r\n    energy: EnergyCard\r\n};\r\n\r\nexport default function ProjectTiles(props) {\r\n\r\n    return (\r\n        <div>\r\n            <header>\r\n            <Container fluid>\r\n                <Row>\r\n                    {\r\n                        props.data.map((project) => {\r\n                            if ((props.type === project.type) || (props.type === 'all')) {\r\n                                const DynamicProjectCard = PROJECT_CARDS[project.name]\r\n\r\n                                return (\r\n                                    <Col lg={true}>\r\n                                        <DynamicProjectCard/>\r\n                                    </Col>\r\n                                );\r\n                            }\r\n                        })\r\n                    }\r\n                </Row>\r\n            </Container>\r\n            </header>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\portfolio\\ProjectThumbnail.js",["140","141"],"import React from 'react';\r\nimport {Card, Button } from 'react-bootstrap'\r\n\r\nimport { useGlobalReducer } from '../../contexts/GlobalContext';\r\nimport { SET_CURRENT_PROJECT } from '../../contexts/GlobalStateReducer'\r\n\r\nexport default function ProjectThumbnail(props) {\r\n    const [ my_state , my_dispatch] = useGlobalReducer()\r\n\r\n    const handleDetailsClick = (project_name) => {\r\n        my_dispatch({type: SET_CURRENT_PROJECT, current_project: project_name})\r\n    }\r\n\r\n    function searchNameIndex(nameKey, myArray){\r\n        for (var i=0; i < myArray.length; i++) {\r\n            if (myArray[i].name === nameKey) {\r\n                return i;\r\n            }\r\n        }\r\n    }\r\n\r\n    const image_src =\r\n        process.env.NODE_ENV === \"development\"\r\n            ? \"http://localhost:3000\"+props.project.thumbnail\r\n            : \"https://uilennest.net/cv\"+props.project.thumbnail\r\n\r\n//: process.env.PUBLIC_URL+props.project.thumbnail\r\n\r\n\r\n    return (\r\n\r\n        <Card className=\"card-img-cutout-dir\" >\r\n            <Card.Img variant top src={image_src} width=\"270\" height=\"270\" />\r\n            <Card.ImgOverlay>\r\n                <h4>{props.project.name}</h4>\r\n                &nbsp;\r\n                &nbsp;\r\n                &nbsp;\r\n                <tr></tr>\r\n                <tr>\r\n                <td>\r\n                    <Button variant=\"outline-warning\" size=\"sm\" onClick={() => handleDetailsClick(props.project.name)}>Details</Button>&nbsp;\r\n                </td>\r\n\r\n                </tr>\r\n            </Card.ImgOverlay>\r\n\r\n        </Card>\r\n\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\whoami\\WhoAmICard.js",[],"R:\\source\\javascript\\my_cv\\src\\routes\\skills\\OlderSkillsCard.js",["142","143","144"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport nico_cv_logo from '../../assets/cv_logo_large.png';\r\n\r\n\r\nexport default function OlderSkillsCard() {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n            <Card>\r\n                <Card.Body>\r\n                    <h2>Older Skills</h2>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={nico_cv_logo} width=\"300\" alt=\"logo cv\"/>\r\n                                    </td>\r\n                                <td align=\"left\">\r\n                            <li>Full Stack Web Developer</li>\r\n                            <li>Java Struts (backend) on Tomcat</li>\r\n                            <li>Java Server Pages (jsp) (frontend)</li>\r\n                            <li>Database: MySQL </li>\r\n                            <li>DevOps: CI/CD pipelines with Jenkins and Subversion (svn)</li>\r\n                            <li>Provisioning: Ansible, Vagrant</li>\r\n                            <li>System Design: SA/SD, Analyses, Functional Design, Technical Design</li>\r\n                            <li>Documentation/Modelling: DFD, UML</li>\r\n                            <li>Async messagebus: qpid</li>\r\n\r\n                            <li>Agile/scrum</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\skills\\SkillsCard.js",["145","146","147"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport nico_cv_logo from '../../assets/cv_logo_large.png';\r\n\r\n\r\nexport default function SkillsCard() {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n            <Card>\r\n                <Card.Body>\r\n                    <h2>Current Skills</h2>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={nico_cv_logo} width=\"300\" alt=\"logo cv\"/>\r\n                                    </td>\r\n                                <td align=\"left\">\r\n                            <li>Full Stack Web Developer (Linux, Windows)</li>\r\n                            <li>Django + DRF (backend), FastAPI (backend)</li>\r\n                            <li>ReactJS (frontend)</li>\r\n                            <li>Database: Postgres, SQlite</li>\r\n                            <li>DevOps: CI/CD pipelines with Gitlab and Github (actions)</li>\r\n                            <li>Containerization: Docker and docker-compose</li>\r\n                            <li>System Design: SA/SD, Analyses, Functional Design, Technical Design</li>\r\n                            <li>Documentation/Modelling: DFD, UML</li>\r\n                            <li>Async messagebus: RabbitMQ, Celery</li>\r\n\r\n                            <li>Agile, scrum, SAFe framework</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\skills\\AncientSkillsCard.js",["148","149","150"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport nico_cv_logo from '../../assets/cv_logo_large.png';\r\n\r\n\r\nexport default function AncientSkillsCard() {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n            <Card>\r\n                <Card.Body>\r\n                    <h2>Ancient Skills</h2>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={nico_cv_logo} width=\"300\" alt=\"logo cv\"/>\r\n                                    </td>\r\n                                <td align=\"left\">\r\n                            <li>Developer (windows)</li>\r\n                            <li>Delphi (GUI)</li>\r\n                            <li>Clipper (dbase3), Pascal, Fortran, Basic</li>\r\n                            <li>Database: dbase3</li>\r\n                            <li>System Design: SA/SD, Analyses, Functional Design, Technical Design</li>\r\n                            <li>Documentation/Modelling: DFD, UML</li>\r\n                            <li>GIS (Geographical Information Systems)</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\welcome\\WelcomeCard.js",[],"R:\\source\\javascript\\my_cv\\src\\routes\\portfolio\\AstroViewCard.js",["151","152","153","154"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport nico_cv_logo from '../../assets/cv_logo_large.png';\r\n\r\n\r\nexport default function AstroViewCard(props) {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n            <Card>\r\n                <Card.Body>\r\n                    <h2 align={\"left\"}>AstroView</h2>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={props.image} width=\"400\" alt=\"project\"/>\r\n                                    </td>\r\n                                <td align=\"left\">\r\n                            <li>ReactJS</li>\r\n                            <li>Team: solo</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\portfolio\\AstroBaseCard.js",["155","156","157","158"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport nico_cv_logo from '../../assets/cv_logo_large.png';\r\n\r\n\r\nexport default function AstroBaseCard(props) {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n            <Card>\r\n                <Card.Body>\r\n                    <h2 align={\"left\"}>AstroBase</h2>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={props.image} width=\"400\" alt=\"project\"/>\r\n                                    </td>\r\n                                <td align=\"left\">\r\n                            <li>Django</li>\r\n                            <li>Team: solo</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\history\\AstronSDCCard.js",["159","160","161"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport sdc from '../../assets/sdc.jpg';\r\n\r\n\r\nexport default function AstronSDCCard() {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n\r\n            <Card>\r\n                <Card.Body>\r\n                    <h3 align={\"left\"}>ASTRON Science Data Center (2020 - present)</h3>\r\n\r\n                    <Card.Text>\r\n\r\n                        <Table>\r\n\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={sdc} width=\"300\" alt=\"logo cv\"/>\r\n                                </td>\r\n                                <td align=\"left\">\r\n                                    In 2019 I joined the 'SDC team' to start development of the ESAP Science Portal for the European Escape project.\r\n                                    I set up the development infrastructure (Gitlab), made the technology choices (Django and ReactJS)\r\n                                    and implemented the first iteration of the ESAP portal and the ADEX spinoff for ASTRON.\r\n                                    <tr>&nbsp;</tr>\r\n                                    In the following years the team grew and became the dedicated development team for the new Science Data Center.\r\n                                    My role changed to a more architectural/design role and lead developer for ADEX.\r\n\r\n                                    <tr>&nbsp;</tr>\r\n                                    <li>ESAP backend & frontend</li>\r\n                                    <li>ADEX backend & frontend (lead engineer)</li>\r\n                                    <li>Architecture, analyses and design</li>\r\n                                    <li>Infrastructure & DevOps (CI/CD pipelines in Gitlab</li>\r\n                                    <li>Introducing documentation vision & strategy</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\history\\AstronROCard.js",["162","163","164"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport lofar from '../../assets/lofar.jpg';\r\n\r\n\r\nexport default function AstronSDCCard() {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n\r\n            <Card>\r\n                <Card.Body>\r\n                    <h3 align=\"left\">ASTRON Radio Observatory, Westerbork (2002 - 2016)</h3>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={lofar} width=\"300\" alt=\"logo cv\"/>\r\n                                </td>\r\n                                <td align=\"left\">\r\n                                    In 2002 I joined ASTRON. Initially I worked on the Cosmolab project for INAF in Bologna\r\n                                    as the ASTRON radio-astronomy representative. This was in C++.\r\n\r\n                                    <tr>&nbsp;</tr>\r\n                                    In 2003 we started to work on MoM for the WSRT telescope, the first web based observation management system.\r\n                                    The technology was Java Struts running in a Tomcat server and was introduced by an external party.\r\n                                    We worked in a team of 4 people on a suite of interconnected software with this technology.\r\n\r\n                                    I also wrote software (in java) to interface the ancient fortran software running on unix\r\n                                    with this newly introduced web application.\r\n\r\n                                    <tr>&nbsp;</tr>\r\n                                    <li>MoM for WSRT Telescope (backend/frontend/databases)</li>\r\n                                    <li>Tomcat Web environment</li>\r\n\r\n                                    <tr>&nbsp;</tr>\r\n                                    In 2007 MoM was ported to the LOFAR telescope. I was now the only developer.\r\n                                    LOFAR was in development for a long time, and my task was to keep MoM adapted accordingly.\r\n\r\n                                    <tr>&nbsp;</tr>\r\n                                    I also wrote the CRAFT software, which was developed to keep track of all the LOFAR hardware components. a micro-service architecture.\r\n                                    This was a micro-services architecture in java, with a java-struts/sql backend on Tomcat\r\n                                    and several distributed java GUI's as services.\r\n                                    The GUI's, combined with a barcode scanner, was used at the factories and the telescope construction sites.\r\n\r\n                                    <tr>&nbsp;</tr>\r\n\r\n                                    <li>MoM for LOFAR Telescope (backend/frontend/databases)</li>\r\n                                    <li>Tomcat Web environment</li>\r\n\r\n                                    <li>CRAFT (backend/frontend developer)</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\history\\AstronApertifCard.js",["165","166","167"],"R:\\source\\javascript\\my_cv\\src\\routes\\portfolio\\EnergyViewCard.js",["168","169","170","171","172"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport nico_cv_logo from '../../assets/cv_logo_large.png';\r\n\r\n\r\nexport default function EnergyViewCard(props) {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n            <Card>\r\n                <Card.Body>\r\n                    <h2 align={\"left\"}>EnergyView (frontend)</h2>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={props.image} width=\"400\" alt=\"image\"/>\r\n                                    </td>\r\n                                <td align=\"left\">\r\n                            <li>ReactJS</li>\r\n                            <li>Team: solo</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\portfolio\\EnergyServerCard.js",["173","174","175","176","177"],"import React from 'react';\r\nimport {Card, Table, Container, Row, Col} from 'react-bootstrap'\r\nimport nico_cv_logo from '../../assets/cv_logo_large.png';\r\n\r\n\r\nexport default function EnergyServerCard(props) {\r\n\r\n    return (\r\n        <div className=\"App\">\r\n            <Card>\r\n                <Card.Body>\r\n                    <h2 align={\"left\"}>Energy Server (backend)</h2>\r\n\r\n                    <Card.Text>\r\n                        <Table>\r\n                            <tr>\r\n                                <td align=\"left\">\r\n                                    <img src={props.image} width=\"400\" alt=\"image\"/>\r\n                                    </td>\r\n                                <td align=\"left\">\r\n                            <li>Django</li>\r\n                            <li>Team: solo</li>\r\n                                </td>\r\n                            </tr>\r\n                        </Table>\r\n                    </Card.Text>\r\n                </Card.Body>\r\n            </Card>\r\n        </div>\r\n    );\r\n\r\n}\r\n\r\n","R:\\source\\javascript\\my_cv\\src\\routes\\history\\GeopsCard.js",["178","179","180"],"R:\\source\\javascript\\my_cv\\src\\routes\\history\\SAVECard.js",["181","182","183"],{"ruleId":"184","replacedBy":"185"},{"ruleId":"186","replacedBy":"187"},{"ruleId":"188","severity":1,"message":"189","line":1,"column":55,"nodeType":"190","messageId":"191","endLine":1,"endColumn":63},{"ruleId":"188","severity":1,"message":"192","line":3,"column":8,"nodeType":"190","messageId":"191","endLine":3,"endColumn":20},{"ruleId":"188","sever